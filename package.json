{
  "name": "yinyang-gen",
  "version": "0.0.7",
  "author": "Felipe Aron",
  "description": "React and React Native component and page generator",
  "bin": {
    "yinyang": "bin/yinyang"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,json} && standard --fix",
    "lint": "standard",
    "test": "jest",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "files": [
    "src",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "gluegun": "^2.1.0",
    "yinyang-gen": "^0.0.5"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "prettier": "^1.12.1",
    "standard": "^12.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "keywords": [
    "react",
    "react native",
    "generator",
    "component",
    "page"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aronfelipe/yinyang-gen"
  }
}
